<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="demo.dao.SupplierDao">
   <insert id="addSuplier" parameterType="Supplier">
       insert into supplier(`supplier`,`commodity`,`address`) value (#{supplier},#{commodity},#{address})
   </insert>
</mapper>